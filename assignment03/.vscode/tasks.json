{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "producer_build",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/main.c",
                "-std=gnu89",
                "-o",
                "${workspaceFolder}/main.o"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        }
    ],
    "version": "2.0.0"
}